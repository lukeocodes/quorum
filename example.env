# PostgreSQL Database Configuration
# These are the default values - only override if your setup differs
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=quorum

# Development mode (optional)
NODE_ENV=development

# Encryption key for storing API keys securely in database
# Generate a random 32-byte hex string for production
# Example: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=your_64_character_hex_encryption_key_here_change_in_production

