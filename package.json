{
  "name": "@quorum/nx",
  "version": "0.1.0",
  "description": "A Slack-like app for discussions between humans and AIs - NX Monorepo",
  "private": true,
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "scripts": {
    "dev": "nx run-many --target=dev --projects=api-core,api-server,api-user,app-electron,app-web,console --parallel=6",
    "dev:api-core": "nx run api-core:dev",
    "dev:api-server": "nx run api-server:dev",
    "dev:api-user": "nx run api-user:dev",
    "dev:apis": "nx run-many --target=dev --projects=api-core,api-server,api-user --parallel=3",
    "dev:app-electron": "nx run-many --target=dev --projects=api-core,api-server,api-user,console,app-electron --parallel=5",
    "dev:app-web": "nx run-many --target=dev --projects=api-core,api-server,api-user,console,app-web --parallel=5",
    "dev:console": "nx run-many --target=dev --projects=api-core,console --parallel=2",
    "build": "nx run-many --target=build --projects=api-core,api-server,api-user,app-electron,app-web,console",
    "build:api-core": "nx run api-core:build",
    "build:api-server": "nx run api-server:build",
    "build:api-user": "nx run api-user:build",
    "build:app-electron": "nx run app-electron:build",
    "build:app-web": "nx run app-web:build",
    "build:console": "nx run console:build",
    "graph": "nx graph",
    "reset": "nx reset"
  },
  "keywords": [
    "monorepo",
    "nx",
    "electron",
    "astro",
    "express",
    "ai",
    "chat",
    "discussion"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@nx/eslint": "^22.1.1",
    "@nx/js": "^22.1.1",
    "@nx/vite": "^22.1.1",
    "@nx/workspace": "^22.1.1",
    "dotenv": "catalog:",
    "nx": "^22.1.1",
    "pg": "catalog:"
  }
}
