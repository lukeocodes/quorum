{
  "name": "api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api/src",
  "projectType": "application",
  "tags": [
    "type:app",
    "platform:node",
    "scope:backend"
  ],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @quorum/api dev",
        "cwd": "{workspaceRoot}"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/apps/api/dist"
      ],
      "options": {
        "command": "pnpm --filter @quorum/api build",
        "cwd": "{workspaceRoot}"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @quorum/api start",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @quorum/api lint",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}

