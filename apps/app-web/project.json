{
  "name": "app-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app-web/src",
  "projectType": "application",
  "tags": [
    "npm:private",
    "npm:astro",
    "npm:react",
    "npm:web",
    "npm:chat",
    "type:app",
    "platform:web",
    "scope:frontend"
  ],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @quorum/app-web dev",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": [
        "^build"
      ]
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/apps/app-web/dist"
      ],
      "options": {
        "command": "pnpm --filter @quorum/app-web build",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": [
        "^build"
      ]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @quorum/app-web preview",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": [
        "build"
      ]
    }
  }
}

