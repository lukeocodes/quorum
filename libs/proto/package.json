{
  "name": "@quorum/proto",
  "version": "0.1.0",
  "description": "Shared types, schemas, and DTOs for Quorum",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "types",
    "typescript",
    "api",
    "proto",
    "schemas"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
