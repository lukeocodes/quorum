{
  "name": "realtime",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/realtime/src",
  "projectType": "library",
  "tags": [
    "npm:private",
    "npm:sse",
    "npm:websocket",
    "type:lib",
    "scope:shared"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/libs/realtime/dist"
      ],
      "options": {
        "command": "tsup src/index.ts --format cjs,esm --dts",
        "cwd": "libs/realtime"
      },
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup src/index.ts --format cjs,esm --dts --watch",
        "cwd": "libs/realtime"
      },
      "dependsOn": [
        "^build"
      ]
    }
  }
}
