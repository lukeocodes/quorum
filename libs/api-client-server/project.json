{
  "name": "api-client-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api-client-server/src",
  "projectType": "library",
  "tags": [
    "npm:public",
    "npm:api",
    "npm:client",
    "npm:http",
    "type:library",
    "scope:shared"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "pnpm run build",
        "cwd": "libs/api-client-server"
      },
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run dev",
        "cwd": "libs/api-client-server"
      },
      "dependsOn": [
        "^build"
      ]
    }
  }
}
